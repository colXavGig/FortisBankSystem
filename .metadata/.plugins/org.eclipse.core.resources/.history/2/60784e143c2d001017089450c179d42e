package client.bankClient.accountManager;

import java.awt.LayoutManager;

import javax.swing.JPanel;
import javax.swing.JLabel;
import javax.swing.JTextField;

public class DepositView extends JPanel {
	private JTextField anount_textField;

	public DepositView() {
		setLayout(null);
		
		JLabel lblNewLabel = new JLabel("New label");
		lblNewLabel.setBounds(49, 37, 46, 14);
		add(lblNewLabel);
		
		anount_textField = new JTextField();
		anount_textField.setBounds(155, 34, 190, 17);
		add(anount_textField);
		anount_textField.setColumns(10);
		// TODO Auto-generated constructor stub
	}
}
